{
  "name": "choicefe",
  "displayName": "Choicefe Notification",
  "description": "Choicefe组件更新通知",
  "version": "1.3.2",
  "publisher": "fanzkday",
  "engines": {
    "vscode": "^1.47.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "icon": "icon.png",
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "keybindings": [
      {
        "command": "extension.exec",
        "key": "ctrl+q q",
        "mac": "cmd+q q"
      }
    ],
    "configuration": [
      {
        "title": "choicefe",
        "properties": {
          "choicefe.components": {
            "type": "array",
            "default": [
              "@choicefe/scm-widgets",
              "@choicefe/scm-permission",
              "@choicefe/scm-components",
              "@choicefe/scm-modal",
              "@choicefe/scm-utils",
              "@choicefe/scm-common",
              "@choicefe/scm-constant"
            ],
            "description": "需要提示更新的组件名字"
          },
          "choicefe.interval": {
            "type": "integer",
            "default": 60,
            "enum": [
              10,
              30,
              60,
              120
            ],
            "description": "默认60分钟更新一次"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/socket.io-client": "^1.4.33",
    "@types/vscode": "^1.47.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "socket.io-client": "^2.3.0"
  }
}
